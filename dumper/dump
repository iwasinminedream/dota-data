    EndAbilityLearnMode(): void;
    IsInAbilityLearnMode(): boolean;
}
interface CDOTA_PanoramaScript_GameUI
{
    /** Control whether the default UI is enabled */
    SetDefaultUIEnabled( nElementType: number, bVisible: boolean ): void;
